/*rank_summary*/
.rank_summary{width: 250px; bottom: 0; right: 20px; z-index: 99999;position: fixed;}
	.rank_summary .header{background: #008080; padding: 10px; color: #fff;border-top-left-radius: 1em;border-top-right-radius: 1em;}
		.rank_summary .header:hover{cursor: pointer;}
		.rank_summary .header span{
			padding-right: 5px;
			float: right;
			position: relative;
			-webkit-animation: anima 2s linear 0.5s infinite alternate; /* Safari 4.0 - 8.0 */
		    animation: anima 2s linear 0.5s infinite alternate;
		}
		/* Safari 4.0 - 8.0 */
		@-webkit-keyframes anima {
		    0% {top:-5px;}
		    25% {top:0px;}
		    50% {top:5px;}
		    75% {top:0px;}
		    100% {top:-5px;}
		}

		/* Standard syntax */
		@keyframes anima {
		    0% {top:-5px;}
		    25% {top:0px;}
		    50% {top:5px;}
		    75% {top:0px;}
		    100% {top:-5px;}
		}

	.rank_summary .content{background: #c6e2ff;background-color: rgba(147,200,255,0.9); padding: 10px;height: 200px;display: none}
